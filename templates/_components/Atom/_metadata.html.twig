<title>{{ title is defined ? title : metadata.seo.ogTitle }}</title>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="{{ metadata.seo.description }}"/>
<meta name="robots" content="index, follow"/>
<link rel="canonical" href="{{ metadata.seo.canonical }}"/>
<meta property="og:locale" content="{{ metadata.seo.ogLocale }}"/>
<meta property="og:type" content="{{ metadata.seo.ogType }}"/>
<meta property="og:title" content="{{ metadata.seo.ogTitle }}"/>
<meta property="og:description" content="{{ metadata.seo.ogDescription }}"/>
<meta property="og:url" content="{{ metadata.seo.ogUrl }}"/>
<meta property="og:site_name" content="{{ metadata.seo.ogSiteName }}"/>
<meta property="og:image:secure_url" content="{{ app.request.uri ~ '/build/media/' ~ metadata.seo.ogImageSecureUrl }}"/>
<meta property="og:image" content="{{ app.request.uri ~ '/build/media/' ~ metadata.seo.ogImageSecureUrl }}"/>
<meta property="og:image:width" content="{{ metadata.seo.ogImageWidth }}"/>
<meta property="og:image:height" content="{{ metadata.seo.ogImageHeight }}"/>

<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:site" content="{{ metadata.seo.canonical }}"/>
<meta name="twitter:title" content="{{ metadata.seo.ogTitle }}"/>
<meta name="twitter:description" content="{{ metadata.seo.ogDescription }}"/>
<meta name="twitter:image" content="{{ app.request.uri ~ '/build/media/' ~ metadata.seo.ogImageSecureUrl }}"/>
